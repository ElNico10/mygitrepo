{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug pa4 (make then run)",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "make-pa4",
      "program": "${workspaceFolder}/pa4",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
  "externalConsole": false,
  "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}